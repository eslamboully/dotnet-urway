<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>DotnetURWay</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Eslamboully.DotnetURWay</PackageId>
    <PackageVersion>1</PackageVersion>
    <Authors>Abdelrahman O Ahmed</Authors>
    <PackageProjectUrl>
        https://github.com/eslamboully/dotnet-urway.git
    </PackageProjectUrl>
    <PackageLicenseExpression>MS-PL</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Package for make urway payment gateway easy to use</PackageReleaseNotes>
    <Copyright>
        copyright Â© 2022 Eslamboully Publishing Limited
    </Copyright>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIconUrl>mainLogo.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
      <None Include="README.md" pack="true" PackagePath="." />
  </ItemGroup>

  <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Configuration" Version="6.0.*" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.*" />
      <PackageReference Include="NuGet.Packaging" Version="6.4.*" />
  </ItemGroup>

  
  
</Project>
